<div class="row">
  <div class="col">
    <h1>Users</h1>
    <% @users.each do |user| %>
      <p><%= link_to user.username, user %></p>
    <% end %>
  </div>
  <div class="col">
    <h1>Images</h1>
    <% @images.each do |image| %>
      <p><%= link_to image.name, image %></p>
    <% end %>
  </div>
</div>
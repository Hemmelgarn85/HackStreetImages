<%-# Created By Michael Hemmelgarn Displays the search results -%>

<div class="container">
<div class="rowsearch">
  <div class="columnsearch">
    <h1>Users</h1>
    <% @users.each do |user| %>
      <p><%= link_to user.username, show_path(user.username) %></p>
    <% end %>
  </div>
  <div class="columnsearch">
    <h1>Images</h1>
    <% @images.each do |image| %>
      <p><%= link_to image.name, image %></p>
    <% end %>
  </div>
</div>
</div>
